{
  "name": "react-tictactoe",
  "version": "3.15.0",
  "description": "React Hooks: Tic Tac Toe",
  "author": "August “Kai” Kaiser <awkaiser@users.noreply.github.com>",
  "repository": "awkaiser/react-tictactoe",
  "homepage": "https://awkaiser.github.io/react-tictactoe/",
  "license": "MIT",
  "private": true,
  "keywords": [
    "tic tac toe",
    "react",
    "minimax"
  ],
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0"
  },
  "devDependencies": {
    "browserslist-config-google": "^3.0.1",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "extends browserslist-config-google"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write"
    ]
  }
}
