    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-jest": "^28.1.1",
    "browserslist-config-google": "^3.0.1",
